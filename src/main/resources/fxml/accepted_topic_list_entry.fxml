<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>


<GridPane hgap="12.0" minHeight="30.0" styleClass="cell" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" percentWidth="60.0" />
      <ColumnConstraints hgrow="SOMETIMES" percentWidth="35.0" />
      <ColumnConstraints hgrow="SOMETIMES" percentWidth="15.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <Label fx:id="resourceLabel" text="Label" />
      <Button fx:id="button1" mnemonicParsing="false" text="X" GridPane.columnIndex="2" />
      <Hyperlink fx:id="link" text="Hyperlink" GridPane.columnIndex="1" />
   </children>
   <padding>
      <Insets bottom="8.0" left="12.0" right="12.0" top="8.0" />
   </padding>
</GridPane>
